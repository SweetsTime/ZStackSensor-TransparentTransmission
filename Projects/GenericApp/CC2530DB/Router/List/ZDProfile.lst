     74          #define FillAndSendBuffer( TRANSSEQ, ADDR, ID, LEN, BUF ) {     \
     75            afStatus_t stat;                                    \
     76            ZDP_TmpBuf = (BUF)+1;                               \
     77            stat = fillAndSend( (TRANSSEQ), (ADDR), (ID), (LEN) );          \
     78            osal_mem_free( (BUF) );                             \
     79            ZDP_TmpBuf = ZDP_Buf+1;                             \
     80            return stat;                                        \
     81          }
     82          
     83          #define FillAndSendTxOptions( TRANSSEQ, ADDR, ID, LEN, TxO ) {  \
     84            afStatus_t stat;                                    \
     85            ZDP_TxOptions = (TxO);                              \
     86            stat = fillAndSend( (TRANSSEQ), (ADDR), (ID), (LEN) );          \
     87            ZDP_TxOptions = AF_TX_OPTIONS_NONE;                 \
     88            return stat;                                        \
     89          }
     90          
     91          #define FillAndSendBufferTxOptions( TRANSSEQ, ADDR, ID, LEN, BUF, TxO ) { \
     92            afStatus_t stat;                                    \
